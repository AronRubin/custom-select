{
  "name": "custom-select",
  "description": "A lightweight JS script for custom select creation.",
  "version": "0.0.10",
  "main": "build",
  "repository": {
    "type": "git",
    "url": "https://github.com/custom-select/custom-select.git"
  },
  "bugs": {
    "url": "https://github.com/custom-select/custom-select/issues"
  },
  "scripts": {
    "clean": "rimraf build && mkdir build && rimraf examples/build && mkdir examples/build",
    "test": "browserify src/test/index.js -o src/test/sauce-bundle.js -t [ babelify ] && node src/test/sauce-bundle.js",
    "prebuild": "npm run clean",
    "build": "babel src/index.js -s -o build/index.js && browserify src/global.js -o build/index.min.js -t [ babelify ] -p [minifyify --no-map] && browserify examples/src/base.js -o examples/build/base.js -t [ babelify ]",
    "prepublish": "npm run clean && npm run test && npm run build"
  },
  "keywords": [
    "select",
    "custom select",
    "html select"
  ],
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "cross-env": "^1.0.8",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.2.0",
    "minifyify": "^7.3.3",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "async": "^2.0.1",
    "browserify": "^13.0.1",
    "custom-event-polyfill": "^0.3.0",
    "sauce-tap-runner": "^0.1.2",
    "tape": "^4.5.1",
    "tape-run": "^2.1.4"
  },
  "author": {
    "name": "Gionatan Lombardi",
    "url": "https://github.com/gionatan-lombardi"
  },
  "contributors": [
    {
      "name": "Marco Nucara",
      "url": "https://github.com/nuky89"
    },
    {
      "name": "Simone Scalbi",
      "url": "https://github.com/pennywiz"
    }
  ],
  "license": "MIT"
}
